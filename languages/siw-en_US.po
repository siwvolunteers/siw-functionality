#: plugins/siw-functionality/data/forms/esc.php:111
msgid "Bevestiging aanmelding"
msgstr "Confirmation application"

#: plugins/siw-functionality/data/forms/begeleider-np.php:133
#: plugins/siw-functionality/data/forms/contact-algemeen.php:50
#: plugins/siw-functionality/data/forms/contact-project.php:50
#: plugins/siw-functionality/data/forms/esc.php:113
#: plugins/siw-functionality/data/forms/infodag.php:88
